{
    "name": "laravel-issue-tracker/authentication",
    "type": "library",
    "description": "This is an authenticatioin package for the Laravel Issue Tracker.",
    "keywords": [
        "laravel-issue-tracker",
        "authentication"
    ],
    "homepage": "https://github.com/laravel-issue-tracker/authentication",
    "license": "MIT",
    "authors": [
        {
            "name": "Istvan Szana",
            "email": "szana8@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : "~5.6|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "LaravelIssueTracker\\Authentication\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelIssueTracker\\Authentication\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    }
}
